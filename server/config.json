{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3443,
  "httpMode": false,
  "certConfig": {
    "path": "./server/certificate",
    "key": "localhost.key",
    "cert": "localhost.cert",
    "ca": [],
    "requestCert": false,
    "rejectUnauthorized": false
  },
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
